<div class="page-header">
  <h1>Listing apn_devices</h1>
</div>

<table>
  <tr>
    <th>Token</th>
    <th></th>
  </tr>

<% @apn_devices.each do |apn_device| %>
  <tr>
    <td><%= link_to apn_device.token, apn_device %></td>
    <td>
      <%= link_to 'Show', apn_device, class: "btn info" %>
      <%= link_to 'Edit', edit_apn_device_path(apn_device), class: :btn %>
      <%= link_to 'Destroy', apn_device, :confirm => 'Are you sure?', :method => :delete %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Device', new_apn_device_path, class: "btn large primary" %>
